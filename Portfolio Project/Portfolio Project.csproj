    <RootNamespace>Portfolio_Project</RootNamespace>
    <UserSecretsId>58bd163e-5e4a-451e-b6b9-a918e6ae93e3</UserSecretsId>
    <AspNetCoreHostingModel>OutOfProcess</AspNetCoreHostingModel>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="wwwroot\logs\**" />
